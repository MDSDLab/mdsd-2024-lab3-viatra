<?xml version="1.0" encoding="ASCII"?>
<model:Main
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:model="webtest.model">
  <testClass>example</testClass>
  <testClass>JavaScriptTest</testClass>
  <declarations
      xsi:type="model:Page"
      name="Bootstrap">
    <variables
        name="largeModal">
      <value
          xsi:type="model:ElementExpression"
          tag="button"
          label="Large modal"/>
    </variables>
    <variables
        name="largeDialog">
      <value
          xsi:type="model:ElementExpression"
          tag="div"
          label="myLargeModalLabel"/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name="ModalDialog">
    <variables
        name="close">
      <value
          xsi:type="model:ElementExpression"
          tag="button"
          label="Close"/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Operation"
      name="HighlightAndClick">
    <parameters
        name="element"/>
    <body>
      <statements
          xsi:type="model:JavaScriptStatement">
        <code
            xsi:type="model:StringExpression"
            value="arguments[0].style.outline = 'red solid 4px'; arguments[0].style.outlineOffset = '-4px'; arguments[0].scrollIntoView({ block: 'center', inline: 'center' });"/>
        <arguments
            xsi:type="model:VariableExpression"
            variable="//@declarations.2/@parameters.0"/>
      </statements>
      <statements
          xsi:type="model:WaitStatement">
        <seconds
            xsi:type="model:IntegerExpression"
            value="1"/>
      </statements>
      <statements
          xsi:type="model:JavaScriptStatement">
        <code
            xsi:type="model:StringExpression"
            value="arguments[0].style.outline = ''; arguments[0].style.outlineOffset = '';"/>
        <arguments
            xsi:type="model:VariableExpression"
            variable="//@declarations.2/@parameters.0"/>
      </statements>
      <statements
          xsi:type="model:ClickStatement">
        <element
            xsi:type="model:VariableExpression"
            variable="//@declarations.2/@parameters.0"/>
      </statements>
    </body>
  </declarations>
  <declarations
      xsi:type="model:TestCase"
      name="largeModalTest">
    <body>
      <statements
          xsi:type="model:OpenStatement">
        <url
            xsi:type="model:StringExpression"
            value="https://getbootstrap.com/docs/4.0/components/modal/"/>
      </statements>
      <statements
          xsi:type="model:ContextStatement"
          page="//@declarations.0">
        <body>
          <statements
              xsi:type="model:CallStatement"
              operation="//@declarations.2">
            <arguments
                  xsi:type="model:VariableExpression"
                  variable="//@declarations.0/@variables.0"/>
          </statements>
          <statements
              xsi:type="model:ContextStatement"
              page="//@declarations.1">
            <element
                xsi:type="model:VariableExpression"
                variable="//@declarations.0/@variables.1"/>
            <body>
              <statements
                  xsi:type="model:CallStatement"
                  operation="//@declarations.2">
                <arguments
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.1/@variables.0"/>
              </statements>
            </body>
          </statements>
        </body>
      </statements>
    </body>
  </declarations>
</model:Main>

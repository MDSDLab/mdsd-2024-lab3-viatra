<?xml version="1.0" encoding="ASCII"?>
<model:Main
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:model="webtest.model">
  <testClass>example</testClass>
  <testClass>WizardTest</testClass>
  <declarations
      xsi:type="model:Page"
      name="Name">
    <variables
        name="firstName">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="First name..."/>
    </variables>
    <variables
        name="lastName">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="Last name..."/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name="ContactInfo">
    <variables
        name="email">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="E-mail..."/>
    </variables>
    <variables
        name="phone">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="Phone..."/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name="Birthday">
    <variables
        name="day">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="dd"/>
    </variables>
    <variables
        name="month">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="mm"/>
    </variables>
    <variables
        name="year">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="yyyy"/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name="LoginInfo">
    <variables
        name="username">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="Username..."/>
    </variables>
    <variables
        name="password">
      <value
          xsi:type="model:ElementExpression"
          tag="input"
          label="Password..."/>
    </variables>
  </declarations>
  <body>
    <statements
        xsi:type="model:OpenStatement">
      <url xsi:type="model:StringExpression"
          value="https://www.w3schools.com/howto/howto_js_form_steps.asp"/>
    </statements>
    <statements
        xsi:type="model:WaitStatement">
      <seconds
          xsi:type="model:IntegerExpression"
          value="1"/>
    </statements>
    <statements
        xsi:type="model:VariableStatement">
      <variable
          name="acceptCookies">
        <value
            xsi:type="model:ElementExpression"
            tag="div"
            label="accept-choices"/>
      </variable>
    </statements>
    <statements
        xsi:type="model:IfStatement">
      <condition
          xsi:type="model:ExistsExpression">
        <operand
            xsi:type="model:VariableExpression"
            variable="//@body/@statements.2/@variable"/>
      </condition>
      <then>
        <statements
            xsi:type="model:ClickStatement">
          <element
              xsi:type="model:VariableExpression"
              variable="//@body/@statements.2/@variable"/>
        </statements>
      </then>
    </statements>
    <statements
        xsi:type="model:WhileStatement">
      <condition
          xsi:type="model:ExistsExpression">
        <operand
            xsi:type="model:ElementExpression"
            tag="button"
            label="Next"/>
      </condition>
      <body>
        <statements
            xsi:type="model:ContextStatement"
            page="//@declarations.0">
          <body>
            <statements
                xsi:type="model:IfStatement">
              <condition
                  xsi:type="model:ExistsExpression">
                <operand
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.0/@variables.0"/>
              </condition>
              <then>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.0/@variables.0"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="Agent"/>
                </statements>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.0/@variables.1"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="Smith"/>
                </statements>
                <statements
                    xsi:type="model:AssertStatement">
                  <condition
                      xsi:type="model:IsExpression">
                    <left
                        xsi:type="model:VariableExpression"
                        variable="//@declarations.0/@variables.1"/>
                    <right
                        xsi:type="model:StringExpression"
                        value="Smith"/>
                  </condition>
                </statements>
              </then>
            </statements>
          </body>
        </statements>
        <statements
            xsi:type="model:ContextStatement"
            page="//@declarations.1">
          <body>
            <statements
                xsi:type="model:IfStatement">
              <condition
                  xsi:type="model:ExistsExpression">
                <operand
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.1/@variables.0"/>
              </condition>
              <then>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.1/@variables.0"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="smith@matrix.com"/>
                </statements>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.1/@variables.1"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="5551234"/>
                </statements>
                <statements
                    xsi:type="model:AssertStatement">
                  <condition
                      xsi:type="model:ContainsExpression">
                    <left
                        xsi:type="model:VariableExpression"
                        variable="//@declarations.1/@variables.0"/>
                    <right
                        xsi:type="model:StringExpression"
                        value="smith"/>
                  </condition>
                </statements>
              </then>
            </statements>
          </body>
        </statements>
        <statements
            xsi:type="model:ContextStatement"
            page="//@declarations.2">
          <body>
            <statements
                xsi:type="model:IfStatement">
              <condition
                  xsi:type="model:ExistsExpression">
                <operand
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.2/@variables.0"/>
              </condition>
              <then>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.2/@variables.0"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="01"/>
                </statements>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.2/@variables.1"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="01"/>
                </statements>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.2/@variables.2"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="2000"/>
                </statements>
              </then>
            </statements>
          </body>
        </statements>
        <statements
            xsi:type="model:ContextStatement"
            page="//@declarations.3">
          <body>
            <statements
                xsi:type="model:IfStatement">
              <condition
                  xsi:type="model:ExistsExpression">
                <operand
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.3/@variables.0"/>
              </condition>
              <then>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.3/@variables.0"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="smith"/>
                </statements>
                <statements
                    xsi:type="model:FillStatement">
                  <element
                      xsi:type="model:VariableExpression"
                      variable="//@declarations.3/@variables.1"/>
                  <value
                      xsi:type="model:StringExpression"
                      value="secret"/>
                </statements>
              </then>
            </statements>
          </body>
        </statements>
        <statements
            xsi:type="model:WaitStatement">
          <seconds
              xsi:type="model:IntegerExpression"
              value="1"/>
        </statements>
        <statements
            xsi:type="model:ClickStatement">
          <element
              xsi:type="model:ElementExpression"
              tag="button"
              label="Next"/>
        </statements>
      </body>
    </statements>
  </body>
</model:Main>

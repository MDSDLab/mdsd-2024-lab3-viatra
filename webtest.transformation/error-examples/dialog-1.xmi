<?xml version="1.0" encoding="ASCII"?>
<model:Main
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:model="webtest.model">
  <testClass>example</testClass>
  <testClass>DialogTest</testClass>
  <declarations
      xsi:type="model:Page"
      name="Bootstrap">
    <variables
        name="largeModal">
      <value
          xsi:type="model:ElementExpression"
          tag="button"
          label="Large modal"/>
    </variables>
    <variables
        name="smallModal">
      <value
          xsi:type="model:ElementExpression"
          tag="button"
          label="Small modal"/>
    </variables>
    <variables
        name="largeDialog">
      <value
          xsi:type="model:ElementExpression"
          tag="div"
          label="myLargeModalLabel"/>
    </variables>
    <variables
        name="smallDialog">
      <value
          xsi:type="model:ElementExpression"
          tag="div"
          label="mySmallModalLabel"/>
    </variables>
    <variables
        name="url1">
      <value
          xsi:type="model:StringExpression"
          value="https://getbootstrap.com/docs/4.0/components/modal/"/>
    </variables>
    <variables
        name="url2">
      <value
          xsi:type="model:StringExpression"
          value="https://getbootstrap.com/docs/4.0/components/modal/"/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name="ModalDialog">
    <variables
        name="close">
      <value
          xsi:type="model:ElementExpression"
          tag="button"
          label="Close"/>
    </variables>
  </declarations>
  <declarations
      xsi:type="model:TestCase"
      name="largeModalTest">
    <body>
      <statements
          xsi:type="model:OpenStatement">
        <url
            xsi:type="model:VariableExpression"
            variable="//@declarations.0/@variables.4"/>
      </statements>
      <statements
          xsi:type="model:ContextStatement"
          page="//@declarations.0">
        <body>
          <statements
              xsi:type="model:ClickStatement">
            <element
                xsi:type="model:VariableExpression"
                variable="//@declarations.0/@variables.0"/>
          </statements>
          <statements
              xsi:type="model:ContextStatement"
              page="//@declarations.1">
            <element
                xsi:type="model:VariableExpression"
                variable="//@declarations.0/@variables.2"/>
            <body>
              <statements
                  xsi:type="model:WaitStatement">
                <seconds
                    xsi:type="model:IntegerExpression"
                    value="2"/>
              </statements>
              <statements
                  xsi:type="model:ClickStatement">
                <element
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.1/@variables.0"/>
              </statements>
            </body>
          </statements>
        </body>
      </statements>
    </body>
  </declarations>
  <declarations
      xsi:type="model:TestCase"
      name="smallModalTest">
    <body>
      <statements
          xsi:type="model:OpenStatement">
        <url
            xsi:type="model:VariableExpression"
            variable="//@declarations.0/@variables.5"/>
      </statements>
      <statements
          xsi:type="model:ContextStatement"
          page="//@declarations.0">
        <body>
          <statements
              xsi:type="model:ClickStatement">
            <element
                xsi:type="model:VariableExpression"
                variable="//@declarations.0/@variables.1"/>
          </statements>
          <statements
              xsi:type="model:ContextStatement"
              page="//@declarations.1">
            <element
                xsi:type="model:VariableExpression"
                variable="//@declarations.0/@variables.3"/>
            <body>
              <statements
                  xsi:type="model:WaitStatement">
                <seconds
                    xsi:type="model:BooleanExpression"
                    value="true"/>
              </statements>
              <statements
                  xsi:type="model:ClickStatement">
                <element
                    xsi:type="model:VariableExpression"
                    variable="//@declarations.1/@variables.0"/>
              </statements>
            </body>
          </statements>
        </body>
      </statements>
    </body>
  </declarations>
</model:Main>

<?xml version="1.0" encoding="ASCII"?>
<model:Main
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:model="webtest.model">
  <testClass>example</testClass>
  <testClass>GoogleTest</testClass>
  <declarations
      xsi:type="model:Operation"
      name="search">
    <parameters
        name="text"/>
    <body>
      <statements
          xsi:type="model:FillStatement">
        <element
            xsi:type="model:ElementExpression"
            tag="textarea"
            label="q"/>
        <value
            xsi:type="model:VariableExpression"
            variable="//@declarations.0/@parameters.0"/>
      </statements>
      <statements
          xsi:type="model:ClickStatement">
        <element
            xsi:type="model:ElementExpression"
            tag="input"
            label="btnK"/>
      </statements>
    </body>
  </declarations>
  <declarations
      xsi:type="model:Page"
      name=""/>
  <body>
    <statements
        xsi:type="model:OpenStatement">
      <url xsi:type="model:StringExpression"
          value="https://www.google.com"/>
    </statements>
    <statements
        xsi:type="model:VariableStatement">
      <variable
          name="acceptCookies">
        <value
            xsi:type="model:ElementExpression"
            tag="button"
            label="L2AGLb"/>
      </variable>
    </statements>
    <statements
        xsi:type="model:IfStatement">
      <condition
          xsi:type="model:ExistsExpression">
        <operand
            xsi:type="model:VariableExpression"
            variable="//@body/@statements.1/@variable"/>
      </condition>
      <then>
        <statements
            xsi:type="model:ClickStatement">
          <element
              xsi:type="model:VariableExpression"
              variable="//@body/@statements.1/@variable"/>
        </statements>
      </then>
    </statements>
    <statements
        xsi:type="model:CallStatement"
        operation="//@declarations.0">
      <arguments
            xsi:type="model:StringExpression"
            value="jwst"/>
    </statements>
    <statements
        xsi:type="model:WaitStatement">
      <seconds
          xsi:type="model:IntegerExpression"
          value="2"/>
      <condition
          xsi:type="model:StringExpression"
          value="true"/>
    </statements>
  </body>
</model:Main>
